  "scripts": {
    "clean": "gatsby clean",
    "test": "cypress open --env LANGUAGE=$GATSBY_LANGUAGE",
    "testci": "cypress run --env LANGUAGE=$GATSBY_LANGUAGE --record --key $CYPRESS_KEY",
    "testcli": "cypress run --env LANGUAGE=$GATSBY_LANGUAGE",
    "test:homepage": "cypress run --env LANGUAGE=$GATSBY_LANGUAGE --spec 'cypress/integration/homepage.spec.js'",
    "test:signup": "cypress run --env LANGUAGE=$GATSBY_LANGUAGE --spec 'cypress/integration/signup.spec.js'",
    "test:login": "cypress run --env LANGUAGE=$GATSBY_LANGUAGE --spec 'cypress/integration/login.spec.js'",
    "test:account": "cypress run --env LANGUAGE=$GATSBY_LANGUAGE --spec 'cypress/integration/account.spec.js'",
    "test:settings": "cypress run --env LANGUAGE=$GATSBY_LANGUAGE --spec 'cypress/integration/settings.spec.js'",
    "test:rights": "cypress run --env LANGUAGE=$GATSBY_LANGUAGE --spec 'cypress/integration/rights.spec.js'",
    "test:patterns": "cypress run --env LANGUAGE=$GATSBY_LANGUAGE --spec 'cypress/integration/patterns.spec.js'",
    "test:measurements": "cypress run --env LANGUAGE=$GATSBY_LANGUAGE --spec 'cypress/integration/measurements.spec.js'",
